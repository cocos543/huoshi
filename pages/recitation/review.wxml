<!--pages/recitation/recite.wxml-->
<view class="header weui-cells weui-cells_after-title">
    <view class="weui-cell">
        <view class="weui-cell__bd">
        <view class="titleTxt">{{titleLabel}}</view>
        <view class="bluetxt bodyTxt">{{topicData.book_name}}</view>
        </view>
        <view class="bluetxt titleTxt weui-cell__ft">忽略</view>
    </view>

    <view class="weui-cell">
        <view class="bodyTxt weui-cell__bd">
        {{handleContent}}
        </view>
    </view>
</view>
<button class="confirmBtn" bindtap="clickDone">我记住了</button>
<button class="gobackBtn" bindtap="clickBack">回去重背</button>